{
  "name": "mapwork",
  "version": "1.0.0",
  "description": "mapwork-legacy",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --no-source-maps --public-url ./ --no-content-hash",
    "test": "jest --coverage",
    "test-ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:once": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/programmerinprogress/mapwork-legacy.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/programmerinprogress/mapwork-legacy#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-junit": "^10.0.0",
    "jquery": "^3.4.1",
    "parcel": "^2.8.2",
    "sass": "^1.23.7"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "jest-canvas-mock",
      "./Scripts/jest.setup.js"
    ],
    "coverageReporters": [
      "text",
      "json",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "output": "output/coverage/junit/junit.xml",
    "usePathForSuiteName": true
  },
  "dependencies": {
    "core-js": "^3.6.1",
    "regenerator-runtime": "^0.13.3"
  }
}
